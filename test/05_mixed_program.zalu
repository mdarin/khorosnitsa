#
# Смешанная прогрмма: функция, ветвления и цикли 
#
func f(a) {
    a = 0
    while (a < 1) {
        if (a > 1) {
            a = a + 1
        } else {
            if (a < 1) {
                x = 0
                while (x < 1) {
                    y = 0
                    while (y < 1) {
                        a = a + 2
                        y = y + 1
                    }
                    x = x + 1
                }
            }
        }
    }
    a
}

n = 0
n = f(n)
n

