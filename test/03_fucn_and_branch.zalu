func f(a) {
    if (a < 1) {
        if (a < 1) {
            if (a > 1) {
                a = a + 1
            } else {
                a = a + 2
                
            }
        }
        if (a >= 2) {
            a = a * 2
        }
    }
    if (a == 4) {
        a = a - 1
    }
    a
}

n = 0
n = f(n)
